{
    "name": "izusoft/log-message",
    "description": "Allows you to create logs in an arbitrary file, works with strings, arrays, objects.",
    "minimum-stability": "stable",
    "license": "MIT",
    "version": "1.1",
    "authors": [
        {
            "name": "Igor Zubarev",
            "email": "zubarevski@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "log-message",
        "laravel-log",
        "laravel",
        "laravel7",
        "log",
        "logger",
        "log-to-file",
        "log-to-channels",
        "logging"
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "monolog/monolog": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "IzuSoft\\Logger\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IzuSoft\\Logger\\LoggerServiceProvider"
            ],
            "aliases": {
                "LogMessage": "IzuSoft\\Logger\\Facades\\LogMessageFacade"
            }
        }
    }
}
